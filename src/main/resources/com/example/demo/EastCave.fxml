        <Button layoutY="284.0" onAction="#toNorthCave" opacity="0.0" prefHeight="108.0" prefWidth="109.0" text="Start Game" />
        <Label fx:id="welcomeText" layoutX="527.0" layoutY="14.0" text="East Cave" textAlignment="CENTER" textFill="WHITE">
